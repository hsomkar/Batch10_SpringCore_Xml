<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans
           http://www.springframework.org/schema/beans/spring-beans-3.0.xsd">

	<bean id="emp" class="com.tyss.springCoreXml.Employee">
	</bean>

	<bean id="empOne" class="com.tyss.springCoreXml.Employee">
		<constructor-arg name="id" value="10" />
		<constructor-arg name="name" value="Sai" />
	</bean>

	<bean id="empTwo" class="com.tyss.springCoreXml.Employee">
		<property name="id" value="20" />
		<property name="name" value="Uday" />
	</bean>

</beans>